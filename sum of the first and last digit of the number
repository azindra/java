import java.util.*;
public class main {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int last_digit=n%10;
        while(n>=10){
            n=n/10;
        }
        int first_number=n;
        System.out.println(last_digit);
        System.out.println(first_number);
        int sum= last_digit+first_number;
        System.out.println("sum"+sum);
}}
